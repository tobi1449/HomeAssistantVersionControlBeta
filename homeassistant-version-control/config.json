{
  "name": "Home Assistant Version Control Beta",
  "version": "1.0.2",
  "slug": "home-assistant-version-control-beta",
  "description": "Git-powered version control and automated backup for your Home Assistant configuration files",
  "url": "https://github.com/diggingfordinos/HomeAssistantVersionControlBeta",
  "startup": "application",
  "boot": "auto",
  "arch": [
    "aarch64",
    "amd64",
    "armhf",
    "armv7",
    "i386"
  ],
  "init": false,
  "hassio_api": true,
  "auth_api": true,
  "homeassistant_api": true,
  "hassio_role": "manager",
  "webui": "http://[HOST]:[PORT:54001]",
  "ingress_port": 54001,
  "ports": {
    "54001/tcp": null
  },
  "map": [
    "config:rw",
    "ssl"
  ],
  "environment": {
    "CONFIG_PATH": "/config"
  },
  "options": {},
  "schema": {},
  "healthcheck": {
    "test": [
      "CMD",
      "node",
      "-e",
      "fetch('http://localhost:54001/').then(r => process.exit(r.ok ? 0 : 1)).catch(() => process.exit(1))"
    ],
    "interval": "5s",
    "timeout": "3s",
    "start_period": "10s",
    "retries": 3
  },
  "codeowners": [
    "@yourusername"
  ],
  "labels": {
    "org.opencontainers.image.title": "Home Assistant Version Control Beta",
    "org.opencontainers.image.description": "Git-powered version control for Home Assistant configuration",
    "org.opencontainers.image.source": "https://github.com/yourusername/havc-addon",
    "org.opencontainers.image.licenses": "MIT"
  },
  "stage": "experimental",
  "supported": false,
  "ingress": true,
  "panel_icon": "mdi:auto-fix",
  "panel_title": "Version Control"
}